public class Character {
    private int hp;
    private int homo;

    public Character(){
        hp=100;
        homo=0;
    }

    public int getHp(){
        return hp;
    }

    public void setHp(int n){
        hp = n;
    }

    public int getHomo(){
        return homo;
    }

    public void setHomo(int n){
        homo = n;
    }

    public void fight(){
    }

    public void fallInLove(){
        setHp(520);
        setHomo(520);
    }

}
